{% extends "base.html.twig" %}

{% form_theme form _self %}
{% block form_row %}
    <div class="form_row form-field">
        <div class="form-label">{{ form_label(form) }}</div>
        <div class="form-input">{{ form_widget(form) }}</div>
        <div class="clearfix"> </div>
        <div class="form-error">{{ form_errors(form) }}</div>
    </div>
{% endblock form_row %}

{% block content %}
    <h2> {{ formHeader }} </h2>
    {{ form_start(form) }}
    {{ form_row(form.cycleId) }}
    {{ form_row(form.tenantId) }}
    {{ form_row(form.penId) }}
    {{ form_row(form.cycleCount) }}
    {{ form_row(form.headCount) }}
    {{ form_row(form.startDate) }}
    {{ form_row(form.endDate) }}
    {{ form_end(form) }}
    
{% endblock %}
